<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="499.0" prefWidth="500.0" styleClass="aurora-background" stylesheets="@css/aurora-style.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="aurora.HistoryController">
 <VBox prefHeight="437.0" prefWidth="459.0" spacing="10">
        <padding>
            <Insets bottom="20" left="20" right="20" top="20" />
        </padding>

        <TableView fx:id="historyTable" prefHeight="364.0" prefWidth="47.0">
            <columns>
                <TableColumn fx:id="regionCol" prefWidth="150" text="Region" />
                <TableColumn fx:id="kpCol" prefWidth="100" text="KP Index" />
                <TableColumn fx:id="timeCol" prefWidth="200" text="Timestamp" />
            </columns>
        </TableView>
        <Button onAction="#goBack" prefHeight="30.0" prefWidth="143.0" text="Back to Dashboard">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font></Button>
    </VBox>
</AnchorPane>
